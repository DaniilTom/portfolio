  <base href="/">
  <script src="~/jquery/jquery.min.js"></script>
  <title>@ViewBag.Title</title>
</head>
<body>
  <div>
    @RenderBody()
  </div>

  <script src="~/dist/runtime-es2015.js" type="module"></script>
